{
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "6.10"
          }
        }]
      ],
      "plugins": [
        "transform-class-properties"
      ]
    },
    "browser": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": "last 2 versions, ie 10-11"
          },
          "modules": false
        }]
      ],
      "plugins": [
        "transform-class-properties"
      ]
    },
    "module": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "6.10"
          },
          "modules": false
        }]
      ],
      "plugins": [
        "transform-class-properties"
      ]
    }
  },
  "sourceMaps": true
}